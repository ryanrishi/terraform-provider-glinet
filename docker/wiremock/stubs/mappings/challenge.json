{
  "request": {
    "method": "POST",
    "url": "/rpc",
    "bodyPatterns": [
      {
        "equalToJson": {
          "method": "challenge",
          "params": {
            "username": "test"
          }
        },
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "challenge.json"
  }
}
